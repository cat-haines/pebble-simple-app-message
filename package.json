{
  "name": "pebble-simple-app-message",
  "author": "MakeAwesomeHappen",
  "version": "1.0.0",
  "files": [
    "dist.zip"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha test/js/spec/ --watch",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/js/spec/ && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100"
  },
  "keywords": [
    "pebble-package",
    "AppMessage"
  ],
  "dependencies": {
    "@smallstoneapps/linked-list": "^1.3.0",
    "pebble-events": "^1.0.2"
  },
  "pebble": {
    "projectType": "package",
    "sdkVersion": "3",
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk"
    ],
    "resources": {
      "media": []
    },
    "messageKeys": [
      "SIMPLE_APP_MESSAGE_CHUNK_DATA",
      "SIMPLE_APP_MESSAGE_CHUNK_SIZE",
      "SIMPLE_APP_MESSAGE_CHUNK_TOTAL"
    ]
  },
  "devDependencies": {
    "eslint": "^1.0.0",
    "eslint-config-pebble": "^1.2.0",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mock-require": "^1.3.0",
    "sinon": "^1.17.4"
  }
}
